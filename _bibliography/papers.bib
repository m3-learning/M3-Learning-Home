  publisher   = {American Physical Society (APS)},
  volume      = 126,
  number      = 11,
  pages       = {117601},
  month       = {19~} # mar,
  year        = 2021,
  url         = {https://journals.aps.org/prl/abstract/10.1103/PhysRevLett.126.117601},
  file        = {All Papers/T/Tückmantel et al. 2021 - Local probe comparison of ferroelectric switchi ... e creep and depinning regimes in Pb(Zr_{0.2}ti_{0.8})O_{3} thin films.pdf},
  copyright   = {https://creativecommons.org/licenses/by/4.0/},
  language    = {en},
  issn        = {0031-9007, 1079-7114},
  pmid        = {33798378},
